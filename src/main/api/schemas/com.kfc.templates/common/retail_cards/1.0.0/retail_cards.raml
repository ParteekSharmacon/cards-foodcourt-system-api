#%RAML 1.0 Library
usage: provides resuable datatypes for card details object

types:
  cards:
   type: object
   properties:
     card_no:
      type: string
      description: card_no
      maxLength: 30
      pattern: ^[a-zA-Z0-9][-a-zA-Z0-9]*$
     card_pin:
      type: string
      description: card_pin
      maxLength: 6
      pattern: ^[a-zA-Z0-9][-a-zA-Z0-9]*$
     card_amt:
      type: number
      maximum: 9999999999
      multipleOf: 0.01
      description: card_amt
